        "server"
    ],
    "devDependencies": {
        "@node-minify/core": "^6.2.0",
        "@node-minify/google-closure-compiler": "^6.2.0",
        "@types/body-parser": "^1.19.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/express-http-proxy": "^1.6.3",
        "@types/node": "^17.0.10",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "sass": "^1.49.0",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@opendocs-editor/authlib": "^1.1.2",
        "adm-zip": "^0.5.9",
        "axios": "^0.25.0",
        "body-parser": "^1.19.1",
        "cookie-parser": "^1.4.6",
        "crypto": "^1.0.1",
        "dev-rest-proxy": "^0.3.0",
        "ejs": "^3.1.6",
        "express": "^4.17.2",
        "express-http-proxy": "^1.6.3",
        "fs": "^0.0.1-security",
        "google-auth-library": "^7.11.0",
        "googleapis": "^92.0.0",
        "http": "^0.0.1-security",
        "mongodb": "^4.3.1",
